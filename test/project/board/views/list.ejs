<!DOCTYPE html>
<html>
  <head>
     <meta charset = "utf-8">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <a href="/write">글쓰기</a><br>
    <br>
    <table width="500" border="1">
        <tr>
           <th>번호</th>
           <th>제목</th> 
           <th>작성자</th> 
           <th>날짜</th> 
           <th>조회수</th> 
        </tr>
        <% data.forEach(function(item, index){%>
            <tr>
                <td><%=item.no%></td>
                <td><%=item.title%></td>
                <td><%=item.name%></td>
                <td><%=item.regdate%></td>
                <td><%=item.hit%></td>
            </tr>
        <%}); %>
    </table>
  </body>
</html>
